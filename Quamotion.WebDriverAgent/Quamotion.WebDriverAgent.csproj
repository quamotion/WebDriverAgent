<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net47;net5.0</TargetFrameworks>
    <VersionPrefix>1.0.0</VersionPrefix>
    <Title>Quamotion iOS WebDriverAgent</Title>
    <Description>A WebDriver server for iOS that runs inside the Simulator.</Description>
    <Product>Quamotion iOS WebDriverAgent</Product>
    <Company>Quamotion</Company>
    <Authors>Facebook, Appium, Quamotion</Authors>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="$(ProjectDir)\WebDriverAgent.zip" />
  </ItemGroup>
</Project>
